<tr>
  <td>
    <img src="<%= user.photo %>" alt="">
    <a href="<%= admin_user_path(user) %>" class="user-link"> <%= user.full_name %></a>
    <span class="user-subhead"><%= user.admin? ? 'Admin' : 'Normal' %></span>
  </td>
  <td>
    <img src="<%= partner.photo %>" alt="">
    <a href="<%= admin_user_path(partner) %>" class="user-link"> <%= partner.full_name %></a>
    <span class="user-subhead"><%= partner.admin? ? 'Admin' : 'Normal' %></span>
  </td>
  <td>
    <%= pair.lunch_date %>
  </td>
  <td style="width: 20%;">
    <a href="<%= admin_mystery_pair_path(pair) %>" class="table-link">
      <span class="fa-stack">
        <i class="fa fa-square fa-stack-2x"></i>
        <i class="fa fa-search-plus fa-stack-1x fa-inverse"></i>
      </span>
    </a>
  </td>
</tr>
